@model Q1.Models.Model
@{
    ViewBag.Title = "ContactRealtorView"; 
}

<h2>Contact Realtor</h2>
<fieldset>
    <legend>Realtor Contact Info</legend>
    <div class="display-label">
        <b>Name</b>
    </div>

    <div class="display-field">
        Richard Howe
    </div>


    <div class="display-label">
        <b>Email</b>
    </div>

    <div class="display-field">
        howe24@gmail.com
    </div>


    <div class="display-label">
        <b>Phone Number</b>
    </div>

    <div class="display-field">
        (123) 456-7890
    </div><br>

    <form class="text-left">
        <label for="name">Your Name</label><br>
        <input type="text" id="name" name="name"><br>
        <label for="password">Your Email</label><br>
        <input type="text" id="email" name="email"><br>
        <label for="phone">Your Phone Number</label><br>
        <input type="text" id="phone" name="phone"><br><br>
        <input type="submit" value="Contact Realtor" />
    </form>
</fieldset>